{
  "name": "career-recommender-backend",
  "version": "1.0.0",
  "description": "Backend API for Career Recommender System",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node utils/seeder.js",
    "seed:import": "node utils/seeder.js -i",
    "seed:delete": "node utils/seeder.js -d",
    "seed-careers": "node scripts/seed-careers.js",
    "seed-careers:force": "node scripts/seed-careers.js --force",
    "seed-all": "node scripts/seed-all.js --force",
    "setup-ml-windows": "cd .. && setup_ml_system_windows.bat",
    "setup-ml-unix": "cd .. && chmod +x setup_ml_system_unix.sh && ./setup_ml_system_unix.sh",
    "train-models": "./ml_system/ml_env/bin/python ml_system/train_models.py",
    "train-models-windows": "cd ml_system && ml_env\\Scripts\\python.exe train_models.py",
    "test": "jest",
    "test-ml": "./ml_system/ml_env/bin/python ml_system/ml_integration_service.py health_check",
    "test-ml-windows": "cd ml_system && ml_env\\Scripts\\python.exe ml_integration_service.py health_check",
    "ml-health": "cd ml_system && ml_env\\Scripts\\python ml_integration_service.py health_check",
    "ml-status": "cd ml_system && dir models 2>nul || echo No models directory found"
  },
  "keywords": [
    "career",
    "recommendation",
    "education",
    "MERN",
    "API"
  ],
  "author": "Career Recommender Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "cloudinary-react": "^1.8.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.4",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-mailjet": "^6.0.8",
    "nodemailer": "^6.9.7",
    "slugify": "^1.6.6",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
